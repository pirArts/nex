<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0"
         metadata-complete="true">
    <display-name>Web Application for ID</display-name>
    <description>A application for test.</description>

    <servlet>
        <servlet-name>GetUser</servlet-name>
        <servlet-class>com.ea.nex.servlet.user.GetUserServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PostUser</servlet-name>
        <servlet-class>com.ea.nex.servlet.user.PostUserServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GetUser</servlet-name>
        <url-pattern>/user/getUser</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PostUser</servlet-name>
        <url-pattern>/user/postUser</url-pattern>
    </servlet-mapping>

</web-app>